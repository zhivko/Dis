<module rename-to="WebUi2">
  <inherits name="com.google.gwt.user.User"/>
  <inherits name="com.google.gwt.logging.Logging"/>
  <inherits name="com.google.gwt.user.theme.chrome.Chrome"/>
  <inherits name="elemental2.dom.Dom"/>
  <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
  <set-configuration-property name="CssResource.style" value="stable"/>
  <entry-point class="si.telekom.dis.client.WebUi2"/>
  <source path="client"/>
  <source path="shared"/>
  <servlet path="/login" class="si.telekom.dis.server.LoginServiceImpl"/>
  <servlet path="/admin" class="si.telekom.dis.server.AdminServiceImpl"/>
  <servlet path="/explorer" class="si.telekom.dis.server.ExplorerServiceImpl"/>
  <public path="resource">
    <include name="*.css"/>
    <include name="*.js"/>
    <include name="/css/*.css"/>
    <include name="/js/*.js"/>
  </public>
</module>