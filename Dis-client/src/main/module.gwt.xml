<module> <!-- rename-to='webui2'> -->
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard' /> -->
	<inherits name="com.google.gwt.logging.Logging" />

	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<inherits name='com.google.gwt.user.theme.chrome.Chrome' />
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->
	<!-- <inherits name='com.tractionsoftware.gwt.user.UTCDateTime' /> -->
	<!-- <inherits name='com.tractionsoftware.gwt.user.UTCDateBox' /> -->
	<inherits name='elemental2.dom.Dom' />

	<inherits name="org.gwtbootstrap3.GwtBootstrap3" />

	<inherits name="org.fusesource.restygwt.RestyGWT" />

	<!-- <inherits name='com.googlecode.gwt.crypto.Crypto' /> -->
	<!-- <inherits name="com.github.gwtbootstrap.Bootstrap" /> -->
	<!-- <inherits name="org.gwtbootstrap3.extras.respond.Respond" /> -->
	<!-- <inherits name="org.gwtbootstrap3.extras.datetimepicker.DateTimePicker" /> -->


	<!-- <inherits name="com.google.gwt.activity.Activity" /> -->
	<!-- <inherits name='com.github.gwtbootstrap.Bootstrap' /> -->
	<!-- <inherits name='com.github.gwtbootstrap.datepicker.Datepicker' /> -->
	<!-- <inherits name='com.github.gwtbootstrap.datetimepicker.Datetimepicker' /> -->
	<!-- <inherits name='com.github.gwtbootstrap.datepicker.Datepicker' /> -->

	<!-- set responsive design -->
	<set-configuration-property name="CssResource.style" value="stable" />

	<!-- Specify the app entry point class. -->
	<entry-point class='si.telekom.dis.client.WebUi2' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

	<servlet class="si.telekom.dis.server.LoginServiceImpl" path="/login" />
	<servlet class="si.telekom.dis.server.AdminServiceImpl" path="/admin" />
	<servlet class="si.telekom.dis.server.ExplorerServiceImpl" path="/explorer" />
	<!-- allow Super Dev Mode -->
	<!-- <add-linker name="xsiframe" /> -->

	<public path='resource'>
		<include name='*.css' />
		<include name='*.js' />
		<include name='/css/*.css' />
		<include name='/js/*.js' />
	</public>

<!-- 	<stylesheet src='/dis.css' /> -->
	<!-- <script src='js/theme.js'/> -->

</module>
